package LevelLoaderTests
import SimpleLevelLoader
import Level_01
import LevelData

@Test function testSimpleLoader()
    let level01 = SimpleLevelLoader.load(LEVEL_01_DATA)

    (level01.getData().getTile(0, 0).pathing == Pathing.BLOCKED).assertTrue()
    (level01.getData().getTile(1, 0).pathing == Pathing.BLOCKED).assertTrue()
    (level01.getData().getTile(0, 1).pathing == Pathing.BLOCKED).assertTrue()
    (level01.getData().getTile(0, 2).pathing == Pathing.BLOCKED).assertTrue()
